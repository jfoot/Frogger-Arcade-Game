<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="WindowPane" maxHeight="800" maxWidth="600" minHeight="800" minWidth="600" prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StartScreenController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="610.0" layoutX="-3.0" pickOnBounds="true">
         <image>
            <Image url="@../application/arcadeBlur.png" />
         </image>
      </ImageView>
      <Button fx:id="test" layoutX="149.0" layoutY="577.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="409.0" />
      <Button fx:id="level1" layoutX="149.0" layoutY="247.0" mnemonicParsing="false" onAction="#level1" prefHeight="89.0" prefWidth="279.0" text="Level 1" />
      <Button fx:id="level2" layoutX="149.0" layoutY="356.0" mnemonicParsing="false" onAction="#level2" prefHeight="89.0" prefWidth="279.0" text="Level 2" />
      <Button fx:id="level3" layoutX="149.0" layoutY="463.0" mnemonicParsing="false" onAction="#level3" prefHeight="89.0" prefWidth="279.0" text="Level 3" />
      <Button id="musicButton" fx:id="musicButton" layoutX="367.0" layoutY="722.0" mnemonicParsing="false" onAction="#musicClick" prefHeight="64.0" prefWidth="218.0" text="Turn Off Music" />
      <ImageView fitHeight="90.0" fitWidth="466.0" layoutX="69.0" layoutY="135.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../application/logo.png" />
         </image>
      </ImageView>
      <Button id="helpButton" layoutX="20.0" layoutY="722.0" mnemonicParsing="false" onAction="#helpClick" prefHeight="64.0" prefWidth="218.0" text="Help Information" />
      <Button fx:id="ScoreBoard1" layoutX="445.0" layoutY="246.0" mnemonicParsing="false" onAction="#level1Score" prefHeight="90.0" prefWidth="86.0" style="-fx-graphic: url('application/score.png')" />
      <Button fx:id="ScoreBoard2" layoutX="445.0" layoutY="355.0" mnemonicParsing="false" onAction="#level2Score" prefHeight="90.0" prefWidth="85.0" style="-fx-graphic: url('application/score.png')" />
      <Button fx:id="ScoreBoard3" layoutX="445.0" layoutY="462.0" mnemonicParsing="false" onAction="#level3Score" prefHeight="90.0" prefWidth="85.0" style="-fx-graphic: url('application/score.png')" />
      <Slider fx:id="LivesSlider" blockIncrement="1.0" layoutX="169.0" layoutY="611.0" majorTickUnit="1.0" max="16.0" min="3.0" minorTickCount="1" onMouseDragged="#dragStarted" prefHeight="40.0" prefWidth="342.0" snapToTicks="true" value="15.0" />
      <Text fx:id="LivesText" fill="WHITE" layoutX="167.0" layoutY="608.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of Lives: 15">
         <font>
            <Font name="Courier New" size="23.0" />
         </font></Text>
  
   </children>
</AnchorPane>
